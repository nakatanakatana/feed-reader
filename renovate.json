{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "github>aquaproj/aqua-renovate-config#2.2.1"
  ],
  "configMigration": true,
  "prConcurrentLimit": 10,
  "prHourlyLimit": 10,
  "platformAutomerge": true,
  "packageRules": [
    {
      "matchManagers": [
        "gomod"
      ],
      "postUpdateOptions": [
        "gomodTidy"
      ]
    }
  ],
  "pinDigests": false
}
